<form accept-charset="UTF-8"
  enctype="multipart/form-data"
  role="form"
  class="col-md-3 col-md-offset-4 photo-upload-form">

  <div class="form-group">
    <label for="photo_file">Choose photos</label><br>
    <input id="photo_file" class="form-control" name="photo[file]" type="file">
  </div>
  
  <div class="form-group">
    <input type="submit"
      class="btn btn-primary btn-lg form-control"
      value="Submit">
  </div>
</form>